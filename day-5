str="python"
s=""
n=len(str)
for i in range(n-1,-1,-1):
    s=s+str[i]
print(s)
#palindrome
n="madam"
s=""
for i in range(4,-1,-1):
        s=s+n[i]
if n==s:
      print("palindrome")
else:
    print("not palindrome")
#
word="education"
count=0
for ch in word:
      if ch=="a" or ch=="e" or ch=="i"or ch=="o" or ch=="u":
        count=count+1

print("vowel count:",count)
