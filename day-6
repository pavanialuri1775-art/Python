#ASCII values
macha="z"
print(ord(macha))
#absolute difference
diff=6-7
if diff<0:
    diff=diff*-1
print(diff)
# 
diff=6-7
print(abs(diff))
#
li=[8,6,0,4,7,3]
sum= 0
for i in li:
    sum=sum+i
print(sum)
#ASCII values
s="hello"
ans=0

for i in range(len(s)-1):
    temp=abs(ord(s[i])-ord(s[i+1]))
    ans=ans+temp

print(ans)
    